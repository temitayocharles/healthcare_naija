# Copy to .env.firebase.local and export before running web/dev deploy commands.
# Never commit real tokens or secrets.

# Firebase web runtime (used by lib/core/config/firebase_config.dart)
export FIREBASE_API_KEY="REPLACE_ME"
export FIREBASE_APP_ID="REPLACE_ME"
export FIREBASE_MESSAGING_SENDER_ID="REPLACE_ME"
export FIREBASE_PROJECT_ID="healthcare-naija"
export FIREBASE_STORAGE_BUCKET="healthcare-naija.firebasestorage.app"

# Optional iOS bundle ID (for docs and runtime metadata only)
export FIREBASE_IOS_BUNDLE_ID="com.example.your.bundle"

# Deploy project IDs
export FIREBASE_PROJECT_ID_STAGING="healthcare-naija-staging"
export FIREBASE_PROJECT_ID_PRODUCTION="healthcare-naija"

# Android app package name used when registering Firebase Android app
# com.nigeriacare.nigeria_health_care

# CI token (local deploy via firebase CLI with --token)
# export FIREBASE_TOKEN="REPLACE_ME"

# Optional AI provider key (not auto-generated)
# export OPENAI_API_KEY="REPLACE_ME"
# export OPENAI_BASE_URL="https://api.openai.com/v1"
# export OPENAI_MODEL="gpt-4o-mini"
# export ENABLE_AI_TRIAGE="true"

# Feature flags (build-time defaults via --dart-define)
# These can be overridden at runtime from Firestore doc: config/feature_flags
# export FF_CHAT_ENABLED="true"
# export FF_CHAT_ATTACHMENTS_ENABLED="true"
# export FF_HEALTH_RECORD_SHARING_ENABLED="true"
# export FF_APPOINTMENT_RESCHEDULE_ENABLED="true"
# export FF_EMERGENCY_LOCATION_SHARE_ENABLED="false"
# export FF_PROVIDER_REVIEWS_ENABLED="false"
# export FF_AI_TRIAGE_ENABLED="false"
# export FF_PAYMENTS_ENABLED="false"
# export FF_ADMIN_CONSOLE_ENABLED="false"
# export FF_MAPS_ENABLED="false"
